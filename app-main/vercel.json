{
  "version": 2,
  "builds": [
    {
      "src": "./main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "env": {
    "API_KEY": "your_api_key_here",
    "ENDPOINT_URL": "https://api.us.inc/hanooman/router/v1/chat/completions",
    "RELEVANCE_THRESHOLD": "1.42"
  }
}
